### End Spacers
### Script
with(between(@,"CONSUMI E LETTURE","Consumi fatturati negli ultimi 12 mesi")) {
  foreach(search_all(@,/(.*Periodo \(dal - al\).+\n.+(?:\n.*\d{2}.\d{2}.\d{4}.+)+)/)) {
    _header=table_header(@,/Consumi fatturati kWh(?: Consumi fatturati kWh)?/,"Energia Reattiva");
    foreach(search_all(@,format(/(.+ - \d\d\.$0.+)/,_mese))) {
      step(table_row(@,_header)) {
        step(captures(@,/(\N) (\N) (\N)/)) {
          %energia_attiva_rilevata[0]+=@;
          %energia_attiva_rilevata[1]+=@;
          %energia_attiva_rilevata[2]+=@;
        }
        %energia_reattiva+=@;
      }
    }
  }
}
