### End Spacers
### Script
if(mese_fattura != mese_fine) {
  # Prossima tabella multimese
  *calc_mese++;
  nexttable();
  goto nome_fornitore;
}
if(@ == "DATI FORNITURA") {
  # Prossima tabella multipod
  *calc_mese = 0;
  *next_page += 1 + *off_dettaglio;
