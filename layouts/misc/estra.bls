### Goto Label ricerca_dettaglio
### Flags TRIM
### Spacers
p+global off_dettaglio;
### End Spacers
### Script
if(ate()) {
  error("Estra: Dettaglio Mancante", global ERRORE_DETTAGLIO_MANCANTE);
} else if(@ != "ELEMENTI DI DETTAGLIO") {
  global off_dettaglio++;
  goto ricerca_dettaglio;
}
