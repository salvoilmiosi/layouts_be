### Box Parsing Dettaglio
### Flags NOREAD
### Page 3
### Rect 0.32258067 0.079041496 0.048387095 0.028506272
### Script
with(global _dettaglio) {
  with(between(@,"TOTALE MATERIA ENERGIA","TOTALE TRASPORTO E GESTIONE CONTATORE")) {
    spesa_materia_energia=sum(matches_num(@,format(/$0 .+ (\N) \N/,_periodo)));
    pcv=search_num(@,format(/Commercializzazione vendita $0 \u20ac\/POD\/mese \N \N (\N)/,_periodo));
    step(captures_num(@,format(/Energia F1 $0 \u20ac\/kWh (\N) (\N)/,_periodo))) {
      prezzo_energia[0]=@;
      energia_attiva[0]=@;
    }
    step(captures_num(@,format(/Energia F2 $0 \u20ac\/kWh (\N) (\N)/,_periodo))) {
      prezzo_energia[1]=@;
      energia_attiva[1]=@;
    }
    step(captures_num(@,format(/Energia F3 $0 \u20ac\/kWh (\N) (\N)/,_periodo))) {
      prezzo_energia[2]=@;
      energia_attiva[2]=@;
    }
    sbilanciamento=search_num(@,format(/Sbilanciamento $0 \u20ac\/kWh (\N)/,_periodo));
  }
  with(between(@,"TOTALE TRASPORTO E GESTIONE CONTATORE","TOTALE ONERI DI SISTEMA")) {
    trasporto_gestione=sum(matches_num(@,format(/$0 .+ (\N) \N/,_periodo)));
    potenza[0:3]=search_num(@,format(/Quota potenza $0 \u20ac\/kW \N (\N)/,_periodo));
  }
  with(between(@,"TOTALE ONERI DI SISTEMA","TOTALE ALTRE IMPOSTE")) {
    oneri=sum(matches_num(@,format(/$0 .+ (\N) \N/,_periodo)));
  }
  with(between(@,"TOTALE ALTRE IMPOSTE","TOTALE FORNITURA")) {
    accise=search_num(@,format(/Imposta erariale di consumo $0 \d+\u00b0 \u20ac\/kWh \N \N (\N)/,_periodo));
  }
  with(between(@,"TOTALE ALTRE PARTITE","TOTALE FORNITURA")) {
    altre_partite=sum(matches_num(@,format(/$0 .+ (\N) \N/,_periodo)));
  }
  with(between(@,"TOTALE FORNITURA","TOTALE DA PAGARE")) {
    iva=search(@,/IVA (\d+%)/);
  }
  imponibile = spesa_materia_energia + trasporto_gestione + oneri + accise + altre_partite;
}
### End Script
### End Box

